pkgname=retroroot-environment
pkgver=1
pkgrel=1
pkgdesc='retroroot script providing common environment variables and functions.'
arch=('any')
url='https://github.com/rr-retroroot'
license=('MIT')
source=('retroroot-env')
md5sums=('SKIP')
groups=('retroroot-toolchain')

package() {
  install -d ${pkgdir}/opt/retroroot
  install -m755 retroroot-env ${pkgdir}/opt/retroroot
}

