pkgname=retroroot-configure
pkgver=1
pkgrel=1
arch=(any)
pkgdesc="retroroot configure wrapper"
depends=('retroroot-environment' 'retroroot-pkg-config')
license=("GPL")
url="https://github.com/PacBrew"
source=("retroroot-configure")
md5sums=('SKIP')
groups=('retroroot-toolchain')

package() {
  install -d "${pkgdir}"/opt/pacbrew/retroroot
  install -m 755 retroroot-configure "${pkgdir}"/opt/pacbrew/retroroot
}

